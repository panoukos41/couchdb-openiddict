{
    "_id": "_design/scope",
    "language": "javascript",
    "views": {
        "count": {
            "reduce": "_count",
            "map": "function (doc) {\n  if (doc.split_discriminator == 'openiddict.scope') {\n    emit(doc._id, doc._rev);\n  }\n}"
        },
        "name": {
            "map": "function (doc) {\n  if (doc.split_discriminator == 'openiddict.scope') {\n    emit(doc.name, doc._rev);\n  }\n}"
        }
    }
}